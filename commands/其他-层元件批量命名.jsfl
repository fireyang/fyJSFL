dom = fl.getDocumentDOM();var timeline = dom.getTimeline();var layers = timeline.getSelectedLayers();if (layers.length != 1){	alert("只能选中一层");}else{	var mcName = prompt("输入批量命名的元件名");	var layer = timeline.layers[layers];	var frames = layer.frames;	for (var i = 0; i < frames.length; i ++)	{		if (frames[i].elements.length > 1)		{			alert("第"+i+"帧有两个以上的元件！");		}else if (frames[i].elements.length > 0)		{			//替换名字			var elem = frames[i].elements[0];			elem.name = mcName;			//fl.trace(elem.name);		}	}}