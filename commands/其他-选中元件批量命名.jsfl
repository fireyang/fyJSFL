dom = fl.getDocumentDOM();var timeline = dom.getTimeline();var mcName = prompt("输入批量命名的元件名");//var layers = timeline.getSelectedLayers()var frames = timeline.getSelectedFrames();//var l = timeline.layres[layers];//fl.trace("frames:"+frames);//fl.trace("layers:"+frames.length);for (var i=0; i<frames.length; i+=3){	var layerIndex = frames[i];	var startFrame = frames[i + 1];	var endFrame = frames[i + 2];	var layer = timeline.layers[layerIndex];	//fl.trace("cl:"+layerIndex);	var layerFrames = layer.frames;	for (var j = startFrame; j < endFrame; j ++)	{		var preFrame = layerFrames[j];		setFrameSelectElemName(preFrame,mcName);	}}//设置帧中的元件名称function setFrameSelectElemName(frame,elemName){	var elems = frame.elements;	for (var i=0; i<elems.length; i++)	{		var elem = elems[i];		//fl.trace("ccc:"+elem.name+"|"+elem.selected);		if (elem.selected)		{			elem.name = elemName;		}	}}