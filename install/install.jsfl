function install(window){	var curVersion = getFlashVersion();	//clear console	fl.outputPanel.clear();	var libPath = fl.scriptURI.replace("install/install.jsfl","");	var configPath = fl.configURI;	//install commands	var commandPath = libPath + "commands/";	//var commandFiles = FLfile.listFolder(commandPath,"files");	var outCommandPath = configPath + "Commands/" + "fyJSFL/";	if ((curVersion == 9))	{		//cs3		outCommandPath = configPath + "Commands/";	}	else	{		if (FLfile.exists(outCommandPath))		{			FLfile.remove(outCommandPath);		}		FLfile.createFolder(outCommandPath);	}	copy(commandPath,outCommandPath);	alert("安装完成！");}install(this);function getFlashVersion(){	var flVersion = fl.version;	var flarr = flVersion.split(" ");	var platform = flarr[0];	var v = flarr[1].split(",");	var curVersion = v[0];	return curVersion;}function copy(srcURI,trgURI,filePre){	//fl.trace(filePre==null);	if ((filePre == null))	{		filePre = "";	}	//fl.trace("a:"+srcURI);	var files = FLfile.listFolder(srcURI,"files");	if ((files != null))	{		for (var i = 0; i < files.length; i++)		{			var infile = srcURI + files[i];			var outfile = trgURI + filePre + files[i];			if (FLfile.exists(outfile))			{				FLfile.remove(outfile);			}			var c = FLfile.copy(infile,outfile);		}	}	var folders = FLfile.listFolder(srcURI,"directories");	if ((folders != null))	{		for (var i = 0; i < folders.length; i++)		{			var inFolder = srcURI + folders[i] + "/";			//var outFolder = trgURI + folders[i]+"/";			filePre = folders[i] + "-";			var outFolder = trgURI;			copy(inFolder,outFolder,filePre);		}	}}