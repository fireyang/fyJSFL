function uninstall(window){	fl.outputPanel.clear();	var curVersion = getFlashVersion();	var configPath = fl.configURI;	var commandPath = configPath + "Commands/";	if ((curVersion < 10))	{		var defaultList = ["Copy Motion as XML.jsfl","Export Motion XML.jsfl","Import Motion XML.jsfl","names.xml"];		//cs3		var files = FLfile.listFolder(commandPath);		for each (var file in files)		{			var isDelete = true;			for each (var out in defaultList)			{				if ((out == file))				{					isDelete = false;					break;				}			}			if (isDelete)			{				var file = commandPath + file;				if (FLfile.exists(file))				{					FLfile.remove(file);				}			}		}	}	else	{		commandPath = commandPath + "fyJSFL/";		if (FLfile.exists(commandPath))		{			FLfile.remove(commandPath);		}	}	alert("卸载完成！");}uninstall(this);function getFlashVersion(){	var flVersion = fl.version;	var flarr = flVersion.split(" ");	var platform = flarr[0];	var v = flarr[1].split(",");	var curVersion = v[0];	return curVersion;}